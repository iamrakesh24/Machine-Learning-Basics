import matplotlib.pyplot as plt

x = [i for i in range(10)]

print(x)

y = [2*i for i in range(10)]

print(y)

plt.xlabel('x-axis')
plt.ylabel('y-axis')
plt.plot(x, y)

plt.xlabel('x-axis') #used for naming the axis
plt.ylabel('y-axis')

plt.scatter(x, y) #used to plot scattered values on the graph
